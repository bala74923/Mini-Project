<link rel="stylesheet" href="/style">

<header>
    <nav class="logo-title">
        <a href="/">
            <img src="/image" alt="logo" class="logo">
            <h1 id="page-title">AcTrak</h1>
        </a>
    </nav>
</header>   
<hr>
<% for(let i=0; i < adminDetails.length; i++) { %>
    <h3 class="centerLink">Event <%= i+1%> </h3>
    <div class="event-box", id="customers">
        <table>
            <tr>
                <th>Admin</th>
                <th><%=adminDetails[i]["name"]%> </th>
            </tr>
            <% for (const det in adminDetails[i]["_doc"]) {%>  
                <%if(det!="__v" & det!="_id" & det!="name" &det!="password"){%> 
                <tr>
                    <td id="event-details-attributes"><%=det%> </td>
                    <td> <%=adminDetails[i]["_doc"][det]%> </td>
                </tr>
                <%}%>
            <%}%>
        </table>
        <hr>
    </div>
<% } %>
<%if(adminDetails.length==0){%>
    <h1>No Admins found</h1>
<% } %>