<header>
    <nav class="logo-title">
        <a href="/">
            <img src="/image" alt="logo" class="logo">
            <h1 id="page-title">AcTrak</h1>
        </a>
    </nav>
</header>   
<hr>
<h1>Past Events</h1>
<% for(let i=0; i < pasteventdetails.length; i++) { %>
    <h3 class="centerLink">Event <%= i+1%> </h3>
    <div class="event-box", id="customers">
        <table>
            <tr>
                <th>title</th>
                <th><%=pasteventdetails[i]["_doc"]["title"]%> </th>
            </tr>
            <% for (const det in pasteventdetails[i]["_doc"]) {%>  
                <%if(det!="__v" & det!="_id" & det!="title"){%> 
                <tr>
                    <td id="event-details-attributes"><%=det%> </td>
                    <td> <%=pasteventdetails[i]["_doc"][det]%> </td>
                </tr>
                <%}%>
            <%}%>
        </table>
        <hr>
    </div>
<% } %>
<%if(pasteventdetails.length==0){%>
    <h1>No past events found</h1>
<% } %>